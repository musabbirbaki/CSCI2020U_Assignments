<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.GridPane?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controller">
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="filenameColumn" editable="false" minWidth="50.0" text="File" />
          <TableColumn fx:id="actualClassColumn" editable="false" minWidth="50.0" prefWidth="75.0" text="Actual Class" />
            <TableColumn fx:id="spamProbabilityColumn" editable="false" minWidth="50.0" prefWidth="75.0" text="Spam Probability" />
        </columns>
      </TableView>
   </children>
</GridPane>
